
pipeline{
    agent any
    tools{
        nodejs 'mynode'
    }

    stages{
        
        stage('built'){
            steps{
            sh 'npm run build'
            }
        }

        stage('test'){
            steps{
                sh 'npm test'
            }
        }

        stage('deploy'){
            steps{
                sh 'npm deploy'
            }
        }
    }
}
